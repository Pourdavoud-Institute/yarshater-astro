{
    "extends": "astro/tsconfigs/strict",
    "include": [".astro/types.d.ts", "**/*", "declarations.d.ts"],
    "exclude": ["dist"],
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@assets/*": ["src/assets/*"],
            "@components/*": ["src/components/*"],
            "@content/*": ["src/content/*"],
            "@data/*": ["src/data/*"],
            "@layouts/*": ["src/layouts/*"],
            "@lib/*": ["src/lib/*"],
            "@styles/*": ["src/styles/*"]
        },
        "jsx": "react-jsx",
        "jsxImportSource": "preact"
    }
}
